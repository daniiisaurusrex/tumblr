	<script type="text/javascript">
	$(document).ready(function () {
	/**  BEGINNING OF IMAGE BACKGROUND
	     Delete this section if you do not want an image background for                  characters.    **/
		$("ul#characters li").each(function () {
			var imageSource = $(this).children("img").attr("src");
			$(this).children(".bg").css("background-image","url(" + imageSource + ")");
		});
		/**     END OF IMAGE BACKGROUND     **/
		var activeFilters = [];
		$("ul#dropdown li").not(".label").addClass("option");
		$("ul#dropdown li:nth-child(2)").addClass("active");
		$("ul#dropdown").click(function () {
			$(this).siblings("#dropdown").children(".option").hide();
			$(this).children(".option").slideToggle("fast");
		});
		$("ul#dropdown li.option").click(function () {
			activeFilters = [];
			$("ul#characters li").show();
			$(this).siblings(".active").removeClass("active");
			$(this).addClass("active");
			label = $(this).text();
			$(this).siblings(".label").text(label);
			$("ul#dropdown li.active").each(function () {
				currentVal = $(this).attr("value");
				activeFilters.push(currentVal);
			});
			for (x = 0; x < activeFilters.length; x++) {
				$("ul#characters li").not("." + activeFilters[x]).hide();
			}
		});
	});
</script>
</body>


